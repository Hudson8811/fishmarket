extends ../layouts/default
include ../helpers/mixins

block variables
	- const title = "FUQ Page"


block content
	.scroll-page
		.section.section--first.vacancy__section
			.container
				.layout
					.layout__col
						.vacancy__section__title.title__52
							!="Менеджер по продажам"
						.vacancy

					.layout__col
						.phone-call
							.phone-call__title.title__32 
								!="ОСТАЛИСЬ ВОПРОСЫ? <br> НАПИШИТЕ ИЛИ ЗАКАЖИТЕ <br> ЗВОНОК"
							a(href="#").btn--box.phone-call__btn.js-phone-call__btn
								.btn.btn--circle
									<svg xmlns="http://www.w3.org/2000/svg" width="11" height="8" viewBox="0 0 11 8" fill="none">
									<path d="M10.0999 4.21591L1.22221 4.11643M6.42527 1.06079L10.0999 4.21591L6.42527 1.06079ZM10.0999 4.21591L6.86969 7.29365L10.0999 4.21591Z" stroke="#07171D" stroke-width="1.33502" stroke-linecap="round" stroke-linejoin="round"/>
									</svg>
								.btn--box__text Заказать звонок
			.contacts__map
				#js-map.contacts__map__item
					//- <iframe src="https://yandex.ru/map-widget/v1/?um=constructor%3Aebeaff5254b284c038cedd4e4eace9ceb6df9e58d1e143167db02be7b369ebb9&amp;source=constructor" width="100%" height="100%" frameborder="0"></iframe>		

		footer.footer.footer__animation.footer__animation--personally.js-footer__animation
			.footer__logo 
				img(src="img/logo__white.png", alt="")
			.footer__title.title__170 
				!="Понимаем море, слышим людей"
			.footer__menu 
				ul.footer__menu__list
					li.footer__menu__list__title Каталог
					li.footer__menu__list__item
						a(href="#") Рыба охлажденная
					li.footer__menu__list__item
						a(href="#") Рыба свежемороженая
					li.footer__menu__list__item
						a(href="#") Филе охлажденное
					li.footer__menu__list__item
						a(href="#") Филе свежемороженое
					li.footer__menu__list__item
						a(href="#") Морепродукты
					li.footer__menu__list__item
						a(href="#") Субпродукты
				ul.footer__menu__list
					li.footer__menu__list__title О компании
					li.footer__menu__list__item
						a(href="#") Производство
					li.footer__menu__list__item
						a(href="#") Склад и логистика
					li.footer__menu__list__item
						a(href="#") мы в социуме
					li.footer__menu__list__item
						a(href="#") карьера в skyfish
					li.footer__menu__list__item
						a(href="#") наша команда
				ul.footer__menu__list
					li.footer__menu__list__title А и еще...
					li.footer__menu__list__item
						a(href="#") контакты
					li.footer__menu__list__item
						a(href="#") Личный кабинет
					li.footer__menu__list__item
						a(href="#") Документы
					li.footer__menu__list__item
						a(href="#") Новости
					li.footer__menu__list__item
						a(href="#") Вопросы и ответы (FAQ)
			a(href="#").btn--box.footer__btn
				.btn.btn--circle
					<svg xmlns="http://www.w3.org/2000/svg" width="11" height="8" viewBox="0 0 11 8" fill="none">
					<path d="M10.0999 4.21591L1.22221 4.11643M6.42527 1.06079L10.0999 4.21591L6.42527 1.06079ZM10.0999 4.21591L6.86969 7.29365L10.0999 4.21591Z" stroke="#07171D" stroke-width="1.33502" stroke-linecap="round" stroke-linejoin="round"/>
					</svg>
				.btn--box__text Подробнее
			.footer__bg
				img(src="img/footer__bg.png", alt="")
			.footer__agreement
				.footer__agreement__date.title__24 © 2023 — 2024
				a(href="#").footer__agreement__text.title__24 
					!="Пользовательское <br>соглашение"